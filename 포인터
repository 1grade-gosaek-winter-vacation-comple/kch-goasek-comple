#include <stdio.h>
#include <stdlib.h>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */
int f(int *x, int *y){
	int temp=0;
	temp=*x;
	*x=*y;
	*y=temp;
}


int main(int argc, char *argv[]) { 
	int a=5, b=10;
	printf("%d %d\n",a,b);
	f(&a,&b);
	printf("%d %d",a,b);
	return 0;
}
